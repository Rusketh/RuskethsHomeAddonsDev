{
  "init": false,
  "slug": "public_folders_dev",
  "name": "Public Folders Development",
  "version": "1.1.15",
  "description": "Development Branch of Public Folders.",
  "stage": "experimental",
  "arch":
  [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "startup": "services",
  "boot": "auto",
  "map":
  [
    "media:ro"
  ],
  "ports":
  {
    "8080/tcp": 8044
  },
  "ports_description":
  {
    "8080/tcp": "Port to serve files on."
  },
  "options":
  {
    "folders":
    [
      {
        "url": "media",
        "path": "/media"
      }
    ],
    "index": ["index.html"],
    "request_logging": false,
    "directory_listing": false
  },
  "schema":
  {
    "folders":
    [
      {
        "url": "str",
        "path": "str",
        "index": ["str"],
        "directory_listing": "bool"
      }
    ],
    "index": ["str"],
    "request_logging": "bool",
    "directory_listing": "bool"
  }
}
